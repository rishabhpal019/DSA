class Solution:
    def zeroFilledSubarray(self, nums: List[int]) -> int:
        result  = 0
        n = len(nums)
        current = 0
        for i in  range(n):
            if nums[i] == 0:
                current += 1
            else:
                result += current * (current + 1) // 2
                current = 0
        result += current * (current + 1) // 2
        return result
        
